{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-function-bind", { "legacy": true}],
    ["@babel/plugin-proposal-decorators", { "legacy": true}],
    "@babel/plugin-proposal-class-properties"
  ]
}
